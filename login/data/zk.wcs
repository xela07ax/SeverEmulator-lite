.zstop {
	position: absolute;
	z-index: 2;
	left: 36px; /*leftWidth*/
	top: 0px;
	height: 19px; /*topHeidht -2 , 1 is border*/
	/*line-height: 20px; lineHeight*/
	overflow: hidden;
	border-top: 1px solid #CFCFCF;
	/*border-bottom: 1px solid #7F9DB9;*/
	background: #FFFFFF none repeat scroll 0%;
}
.zscornertop{
	position: absolute;
	left: 36px; /*leftWidth*/
	top: 0px;
	height: 19px; /*topHeidht -2 , 1 is border*/
	overflow: hidden;
	border-top: 1px solid #CFCFCF;
	background: #FFFFFF none repeat scroll 0%;
}


.zstopi, .zscornertopi{
	position: relative;
	top: 0px;
	left: 0px;
	height: 100%;
	display: block;
	margin: 0px;
	padding: 0px;
	overflow-y: visible;
	white-space: nowrap;
}

.zstophead {
	overflow: visible;
}

.zstopblock {
	left: 0px;
	top: 0px;
	display: block;
	position: relative;
	overflow: visible;
	background-color:#FFFFFF;
}

.zstopcell {
	display: -moz-inline-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	border-right: 1px solid #CFCFCF;
	border-bottom: 1px solid #BBBBBB;
	padding: 0px 2px 0px 2px; /*cellPadding*/
	height: 19px; /*topHeigth*/
	width: 64px; /*cellWidth*/
	overflow: hidden;
	vertical-align: top;
	/*line-height: 20px; lineHeigth*/
	background: #FFFFFF;
	overflow: visible;
	position:relative;
	cursor : default;
	display: inline-block;
	color: #5E5E5E;
}

.zstopcelltxt {
	font-size: 12px;
	text-align: center;
	width: 49px;  /* colWidth - 2*cellPadding - 1 , 1 is border*/
	overflow: hidden;
}

.zsleft{
	position: absolute;
	z-index: 2;
	top: 20px; /* topHeight */
	left: 0px;
	width: 35px; /* leftWith - 2 , 2 is border */
	overflow: hidden;
	border-left: 1px solid #CFCFCF;
	background: #CFCFCF none repeat scroll 0%;
}

.zscornerleft  {
	position: absolute;
	z-index: 2;
	top: 20px; /* topHeight */
	left: 0px;
	width: 34px; /* leftWith - 2 , 2 is border */
	overflow: hidden;
	border-left: 1px solid #CFCFCF;
	background: #FFFFFF none repeat scroll 0%;
}

.zslefti, .zscornerlefti {
	position: relative;
	top: 0px;
	left: 0px;
}


.zsleftblock {
	left: 36px;
	top: 0px;
	display: block;
	position: absolute;
	overflow: visible;
	background-color:#FFFFFF;
}

.zslefthead {
	background-color: #FFFFFF;
	overflow: visible;
}

.zsleftcell {
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	border-right: 1px solid #BBBBBB;
	/*line-height: 20px;*/
	height: 19px; /* rowHeight - 1, 1 is border */
	border-bottom: 1px solid #CFCFCF;
	text-align: center;
	overflow: visible;
	cursor : default;
	color: #5E5E5E;
}
.zsleftcelltxt {
	font-size: 12px;
	/* height: 19px;*/ /* rowHeight - 1, 1 is border */
	text-align: center;
	
	overflow: hidden;
}

.zstop-sel,
.zsleft-sel {
	background-color: #E1E1E1;
	color: #008DB7;
	font-weight: bold;
}

/* Header Selected bar */
.zstop-sel:after,
.zsleft-sel:after {
	content: '';
	background: #008DB7;
	position: absolute;
}

.zstop-sel:after {
	bottom: -1px;
	left: 0;
	height: 2px;
	width: 100%;
	border-right: 1px solid #008DB7;
}

.zsleft-sel:after {
	top: 0;
	right: -1px;
	width: 2px;
	height: 100%;
	border-bottom: 1px solid #008DB7;
}

